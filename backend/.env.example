# Port API Configuration
# Client Credentials (Recommended for Production)
PORT_CLIENT_ID=your_client_id_here
PORT_CLIENT_SECRET=your_client_secret_here

# Alternative: Manual Tokens (Legacy)
# PORT_API_TOKEN_PRIMARY=your_primary_token_here
# PORT_API_TOKEN_SECONDARY=your_backup_token_here
# PORT_SERVICE_TOKEN=your_service_token_here

# Optional Configuration
PORT_API_REGION=us
# PORT_API_REGION options: us, eu, us-api, eu-api

# Server Configuration
PORT=8000
